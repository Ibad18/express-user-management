{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.mjs", 
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "backend/app.mjs"
      },
      {
        "src": "/(.*)", 
        "dest": "/frontend/$1"
      }
    ]
  }
  